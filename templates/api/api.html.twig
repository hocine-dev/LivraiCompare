curl -X GET "https://LivraiCompare.dz/api/get-tarifs" \
-H "X-API-KEY: Bearer YOUR_API_KEY" \
-H "Content-Type: application/json" \
-d '{
    "origin": {
        "wilaya": "16",
        "commune": "1601",
        "zone": "A"
    },
    "destination": {
        "wilaya": "15",
        "commune": "1501",
        "zone": "D"
    },
    "package": {
        "weight": "2 kg",
        "value": "2000 DZD"
    },
    "delivery": {
        "mode": "domicile",
        "express": "Oui",
        "urgency": "two-day"
    }
}'</pre>
